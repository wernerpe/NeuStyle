    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train NeuS",
            "type": "python",
            "request": "launch",
            "module": "src.neus.main",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        },
        {
            "name": "Render NeuS",
            "type": "python",
            "request": "launch",
            "module": "src.neus.main",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "mode=render",
                "rendering.checkpoint=results/checkpoints/mic.ckpt",
                "rendering.deformation=results/meshes_stylized/3.0/mic.pickle",
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        },
        {
            "name": "Visualize Epipolar Lines",
            "type": "python",
            "request": "launch",
            "module": "src.neus.scripts.visualize_epipolar_lines",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        },
        {
            "name": "Run Cubic Stylization",
            "type": "python",
            "request": "launch",
            "module": "src.cubicstylization.main",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "input=results/meshes/teddy.stl",
                "output=results/meshes_stylized/teddy.stl",
                "output_package=results/meshes_stylized/teddy.pickle",
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        },
    ]
}